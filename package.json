{
  "name": "alexa-coloring-book",
  "version": "1.0.0",
  "description": "Alexa skill for creating coloring books through voice interaction",
  "main": "dist/index.js",
  "scripts": {
    "setup-node": "./scripts/setup-node.sh",
    "build": "./node_modules/.bin/tsc",
    "start": "node dist/index.js",
    "test": "./node_modules/.bin/jest",
    "test:unit": "./node_modules/.bin/jest --testPathIgnorePatterns=integration",
    "test:integration": "./node_modules/.bin/jest --testPathPattern=integration",
    "test:all": "./node_modules/.bin/jest --coverage",
    "test:skill": "./scripts/test-skill.sh",
    "deploy": "./node_modules/.bin/ask deploy",
    "deploy:infra": "./infrastructure/deploy.sh deploy",
    "deploy:lambda": "./scripts/deploy-lambda.sh",
    "deploy:dev": "npm run deploy:infra dev && npm run deploy:lambda dev && npm run skill:configure dev",
    "deploy:prod": "npm run deploy:infra prod && npm run deploy:lambda prod && npm run skill:configure prod",
    "deploy:complete": "npm run deploy:infra && npm run deploy:lambda && npm run skill:create",
    "health-check": "./scripts/health-check.sh",
    "infra:status": "./infrastructure/deploy.sh status",
    "infra:outputs": "./infrastructure/deploy.sh outputs",
    "infra:delete": "./infrastructure/deploy.sh delete",
    "skill:configure": "./scripts/configure-skill.sh configure",
    "skill:create": "./scripts/configure-skill.sh create",
    "skill:deploy": "./scripts/configure-skill.sh deploy",
    "skill:update": "./scripts/configure-skill.sh update",
    "skill:status": "./scripts/configure-skill.sh status",
    "skill:test": "./scripts/configure-skill.sh test",
    "skill:enable": "./scripts/configure-skill.sh enable",
    "skill:summary": "./scripts/configure-skill.sh summary",
    "models:build": "node scripts/build-interaction-models.js",
    "models:deploy": "node scripts/deploy-interaction-models.js",
    "models:update": "npm run models:build && npm run models:deploy",
    "models:add-values": "node scripts/build-interaction-models.js --add-values",
    "models:add-samples": "node scripts/build-interaction-models.js --add-samples",
    "models:test": "node scripts/test-dynamic-system.js",
    "models:diagnose": "node scripts/diagnose-intent-issues.js",
    "lint": "./node_modules/.bin/eslint src/**/*.ts",
    "validate-npx-setup": "./scripts/validate-npx-setup.sh",
    "validate-deployment": "./scripts/validate-deployment.sh"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.450.0",
    "@aws-sdk/client-dynamodb": "^3.450.0",
    "@aws-sdk/client-s3": "^3.450.0",
    "@aws-sdk/lib-dynamodb": "^3.450.0",
    "@aws-sdk/s3-request-presigner": "^3.450.0",
    "ask-cli": "^2.30.7",
    "ask-sdk-core": "^2.13.0",
    "ask-sdk-model": "^1.29.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "qrcode": "^1.4.4",
    "uuid": "^7.0.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^18.19.0",
    "@types/qrcode": "^1.5.5",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.9.2"
  },
  "keywords": [
    "alexa",
    "skill",
    "coloring-book",
    "children",
    "ai",
    "bedrock"
  ],
  "author": "Alexa Coloring Book Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}